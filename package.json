{
  "name": "codewhisper",
  "type": "module",
  "version": "1.10.0",
  "description": "A powerful tool for converting repository code to AI-friendly prompts",
  "author": "Gordon Mickel <gordon@mickel.tech>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/gmickel",
  "homepage": "https://github.com/gmickel/CodeWhisper",
  "repository": {
    "type": "git",
    "url": "https://github.com/gmickel/CodeWhisper.git"
  },
  "bugs": "https://github.com/gmickel/CodeWhisper/issues",
  "keywords": [
    "code-analysis",
    "ai-tools",
    "developer-tools",
    "code-summarization",
    "static-analysis",
    "llm-integration",
    "documentation-generator",
    "code-review",
    "cli-tool",
    "github-actions",
    "ci-cd",
    "codebase-analysis",
    "ai-prompts",
    "code-intelligence",
    "productivity",
    "code-to-prompt",
    "ai-code-analysis",
    "automated-documentation",
    "code-assistant",
    "ai-code-assistant",
    "ai-code-generator",
    "ai-code-generation",
    "devtools",
    "automated-code-review",
    "automated-code-analysis",
    "automated-code-generation",
    "code-assistance",
    "code-generator",
    "code-generation"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    },
    "./core": {
      "types": "./dist/core/file-worker.d.ts",
      "import": "./dist/core/file-worker.js"
    },
    "./utils/worker-path": {
      "types": "./dist/utils/worker-path.d.ts",
      "import": "./dist/utils/worker-path.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["./dist/*", "./dist/index.d.ts"]
    }
  },
  "bin": {
    "codewhisper": "./dist/cli.js"
  },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "prebuild": "pnpm run typecheck",
    "build": "tsup && npm run fixPermissions",
    "lint": "biome check .",
    "format": "biome format --write .",
    "lint:fix": "biome check . --write",
    "prepublishOnly": "pnpm run build",
    "prerelease": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "semantic-release",
    "fixPermissions": "chmod +x ./dist/cli.js",
    "dev": "NODE_ENV=development esno src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "0.0.34",
    "@ai-sdk/openai": "0.0.40",
    "@anthropic-ai/sdk": "0.25.0",
    "@inquirer/prompts": "5.3.6",
    "@octokit/rest": "21.0.1",
    "@types/diff": "5.2.1",
    "ai": "3.2.45",
    "chalk": "5.3.0",
    "commander": "12.1.0",
    "diff": "5.2.0",
    "dotenv": "16.4.5",
    "fast-glob": "3.3.2",
    "fs-extra": "11.2.0",
    "gpt-tokenizer": "2.2.1",
    "handlebars": "4.7.8",
    "ignore": "5.3.1",
    "inquirer": "9.2.23",
    "inquirer-file-tree-selection-prompt": "2.0.5",
    "isbinaryfile": "5.0.2",
    "micromatch": "4.0.7",
    "ollama-ai-provider": "0.10.0",
    "ora": "8.0.1",
    "piscina": "4.6.1",
    "simple-git": "3.25.0",
    "strip-comments": "2.0.1",
    "winston": "3.13.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/inquirer": "9.0.7",
    "@types/micromatch": "4.0.9",
    "@types/node": "20.14.13",
    "@types/strip-comments": "2.0.4",
    "@vitest/coverage-v8": "2.0.5",
    "@vitest/ui": "2.0.5",
    "bumpp": "9.4.1",
    "esno": "4.7.0",
    "lefthook": "1.7.11",
    "semantic-release": "24.0.0",
    "tsup": "8.2.3",
    "typescript": "5.5.4",
    "vite": "5.3.5",
    "vitest": "2.0.5"
  },
  "packageManager": "pnpm@9.6.0",
  "workspaces": ["apps/*", "packages/*"],
  "trustedDependencies": ["@biomejs/biome", "lefthook"],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
